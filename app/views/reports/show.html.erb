<div class="mdl-tabs mdl-js-tabs mdl-js-ripple-effect">
      <div class="mdl-tabs__tab-bar">
      	<a href="#lannister-panel" class="mdl-tabs__tab  is-active">ANKET BİLGİLERİ</a>
          <a href="#starks-panel" class="mdl-tabs__tab">Sorular</a>
          <a href="#targaryens-panel" class="mdl-tabs__tab ">Giriş Kodları</a>
      </div>

      <div class="mdl-tabs__panel  is-active" id="lannister-panel">
      </div>
       <div class="mdl-tabs__panel" id="starks-panel">
       		<%@poll.questions.each do |pq|%>
       			<%=pq.question_header%>
       			
       		<%end%>
      </div>
      <div class="mdl-tabs__panel" id="targaryens-panel">
          <table class="mdl-data-table mdl-js-data-table">
          <thead>
            <tr>
              <th class="mdl-data-table__cell--non-numeric">Giriş Kodları</th>
            </tr>
          </thead>
          <tbody>
          <div class="digg_pagination">
            <%= will_paginate @poll_passcodes%>
              <%@poll_passcodes.each do |ps|%>
              <tr>
                <td class="mdl-data-table__cell--non-numeric"><%=link_to ps.passcode,reportpasscode_path(:id => ps)%></td>
              </tr>
            <% end %>
          </div>
   
          </tbody>
        </table>
      </div>

